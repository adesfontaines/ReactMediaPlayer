{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MusicPlayerDb": "Data Source=music-player.db"
  },
  "pwa": {
    "cacheId": "Worker 1.0",
    "strategy": "cacheFirstSafe",
    "offlineRoute": "./ClientApp/public/index.html",
    "registerServiceWorker": true,
    "registerWebmanifest": true
  }
}
